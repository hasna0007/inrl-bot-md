    "AUDIO_DATA": {
      "description": "audio metadata",
      "required": true,
      "value": "𝐂𝚫𝚻 𝐒𝚸𝐘;𝛭𝑈𝛨𝛥𝛭𝛭𝛯𝐷;https://telegra.ph/file/f1ae2cd1d9d934d9c2465.jpg"
